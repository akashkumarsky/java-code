package string;

import java.util.Arrays;

public class Q3find_dublicate {
	public static boolean checkdublicate(String str) {
		char [] ch = str.toCharArray();
		Arrays.sort(ch);
		int i =0;
		int j = i+1;
		while(i<j) {
			if(ch[i++]!=ch[j++]);
		}
		return false;
	}
	public static void main(String [] args) {
		String str = "appa";
		checkdublicate(str);
	}
}
