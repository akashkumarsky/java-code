package MAY11;

public class ZeroOneTwo {
	public static void saprate(int[] arr) {

		int size = arr.length;
		int[] a = new int[size];
		int zeroCount = 0;
		int oneCount = 0;
		int twoCount = 0;
		for (int i = 0; i < size; i++) {
			if (arr[i] == 0) {
				zeroCount++;
			} else if (arr[i] == 1) {
				oneCount++;
			} else if (arr[i] == 2) {
				oneCount++;
			}

		}

	}

	public static void main(String[] args) {
		int[] arr = { 1, 0, 0, 1, 2, 1, 1, 2, 2, 2, 2, 1, 1, 1, 0, 0, 0 };
		ZeroOneTwo.saprate(arr);

	}
}
