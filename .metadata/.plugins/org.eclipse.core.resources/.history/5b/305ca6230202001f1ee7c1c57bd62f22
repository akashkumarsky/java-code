package linkedlist;

class Node {
	int val;
	Node next;

	Node(int val) {
		this.val = val;
	}
}

public class Q2ReV_LL_Group_size_t {
	static Node tReverse(Node head, int t) {
		Node temp = head;
		Node prevlast= null;
		while
