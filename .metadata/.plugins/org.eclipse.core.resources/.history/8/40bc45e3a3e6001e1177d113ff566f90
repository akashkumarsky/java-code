package string;

public class Q2Pelindrome {
	public static boolean isPelindrome(String str) {
	int j = str.length()-1;
		for(int i =0; i<str.length(); i++) {
			if(str.charAt(i)== str.charAt(j--)){
				return true;
				
			}
		}
		
		return false;
	}
	public static void main(String[] args) {
		String str = "appa";
		isPelindrome(str);
	}
}
