package pratice;

public class bubbleSort {
	public static void main(String[] args) {
		int[] a = new int[] { 1, 45, 66, 656, 76, 2, 4, 6 };
		bubble(a);

	}

	private static void bubble(int[] a) {
		for (int i = 0; i < a.length; i++) {
			if(a[i]>a[i+1]) {
				int temp = a[i];
				a[i]=a[i+1];
				a[i+1];
				
			}
		}
		
	}
}
