package java450;

public class Q12Merge_two_sorted_Arrays {
	public static int [] mergeSort(int [] arr1, int [] arr2) {
		int n = arr1.length;
		int m = arr2.length;
		int i =m;
		for(int j =0; j<n;j++) {
			arr1[i]=arr2[i];
			i++;
		}
		
		return arr1;
	}
	public static void main(String[] args) {
		int [] arr1 = {1,3,5,6,7};
		int [] arr2 = {2,4,6,6,73};
		mergeSort(arr1,arr2);
	}

}
